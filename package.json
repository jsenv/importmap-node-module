{
  "name": "@jsenv/importmap-node-module",
  "version": "7.0.1",
  "description": "Generate importmap for node_modules",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jsenv/importmap-node-module"
  },
  "bin": "./src/cli.mjs",
  "engines": {
    "node": ">=16.17.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./src/main.js",
  "exports": {
    ".": {
      "import": "./src/main.js"
    },
    "./*": "./*"
  },
  "files": [
    "/src/"
  ],
  "workspaces": [
    "./docs/demo"
  ],
  "volta": {
    "node": "22.3.0"
  },
  "scripts": {
    "eslint": "npx eslint .",
    "test": "node ./scripts/test.mjs",
    "prettier": "prettier --write .",
    "performance": "node --expose-gc ./scripts/performance.mjs --log"
  },
  "dependencies": {
    "@babel/core": "7.26.7",
    "@babel/traverse": "7.26.7",
    "@jsenv/ast": "6.5.0",
    "@jsenv/filesystem": "4.13.4",
    "@jsenv/importmap": "1.2.1",
    "@jsenv/logger": "4.1.1",
    "@jsenv/urls": "2.6.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-proposal-decorators": "7.25.9",
    "@babel/plugin-syntax-import-attributes": "7.26.0",
    "@babel/plugin-syntax-jsx": "7.25.9",
    "@jsenv/assert": "4.4.2",
    "@jsenv/core": "39.13.3",
    "@jsenv/github-release-package": "1.6.18",
    "@jsenv/importmap-eslint-resolver": "5.2.5",
    "@jsenv/package-publish": "1.11.18",
    "@jsenv/performance-impact": "4.4.18",
    "@jsenv/server": "15.5.1",
    "@jsenv/snapshot": "2.11.34",
    "@jsenv/test": "3.5.41",
    "eslint": "9.19.0",
    "@jsenv/eslint-config-relax": "1.3.13",
    "prettier": "3.4.2",
    "prettier-plugin-organize-imports": "4.1.0"
  }
}
