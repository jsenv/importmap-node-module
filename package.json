{
  "name": "@jsenv/importmap-node-module",
  "version": "5.5.4",
  "description": "Generate importmap for node_modules",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jsenv/importmap-node-module"
  },
  "bin": "./src/cli.mjs",
  "engines": {
    "node": ">=16.17.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./src/main.js",
  "exports": {
    ".": {
      "import": "./src/main.js"
    },
    "./*": "./*"
  },
  "files": [
    "/src/"
  ],
  "workspaces": [
    "./docs/demo"
  ],
  "scripts": {
    "eslint": "npx eslint . --ext=.js,.mjs,.cjs,.html",
    "performance": "node --expose-gc ./scripts/performance.mjs --log",
    "test": "node ./scripts/test.mjs",
    "test:coverage": "npm run test -- --coverage",
    "prettier": "prettier --write ."
  },
  "dependencies": {
    "@babel/core": "7.23.2",
    "@babel/traverse": "7.23.2",
    "@jsenv/filesystem": "4.3.2",
    "@jsenv/importmap": "1.2.1",
    "@jsenv/logger": "4.1.1",
    "@jsenv/urls": "2.2.1",
    "parse5": "7.1.2"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.22.15",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-proposal-decorators": "7.23.2",
    "@babel/plugin-syntax-jsx": "7.22.5",
    "@babel/plugin-syntax-import-assertions": "7.22.5",
    "@jsenv/assert": "2.11.0",
    "@jsenv/babel-preset": "1.1.3",
    "@jsenv/core": "38.2.10",
    "@jsenv/eslint-config": "16.4.4",
    "@jsenv/github-release-package": "1.5.3",
    "@jsenv/importmap-eslint-resolver": "5.2.5",
    "@jsenv/package-publish": "1.10.3",
    "@jsenv/performance-impact": "4.1.3",
    "@jsenv/server": "15.1.3",
    "@jsenv/test": "1.7.2",
    "eslint": "8.51.0",
    "eslint-plugin-html": "7.1.0",
    "eslint-plugin-import": "2.28.1",
    "prettier": "3.0.3"
  }
}
