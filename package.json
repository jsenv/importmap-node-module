{
  "name": "@jsenv/importmap-node-module",
  "version": "7.2.2",
  "description": "Generate importmap for node_modules",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jsenv/importmap-node-module"
  },
  "bin": "./src/cli.mjs",
  "engines": {
    "node": ">=16.17.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./src/main.js",
  "exports": {
    ".": {
      "import": "./src/main.js"
    },
    "./*": "./*"
  },
  "files": [
    "/src/"
  ],
  "workspaces": [
    "./docs/demo"
  ],
  "volta": {
    "node": "23.11.0"
  },
  "scripts": {
    "eslint": "npx eslint .",
    "test": "node ./scripts/test.mjs",
    "prettier": "prettier --write .",
    "performance": "node --expose-gc ./scripts/performance.mjs --log"
  },
  "dependencies": {
    "@babel/core": "7.27.4",
    "@babel/traverse": "7.27.4",
    "@jsenv/ast": "6.7.6",
    "@jsenv/filesystem": "4.15.4",
    "@jsenv/importmap": "1.2.2",
    "@jsenv/logger": "4.1.1",
    "@jsenv/urls": "2.9.0",
    "is-unicode-supported": "2.1.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-proposal-decorators": "7.27.1",
    "@babel/plugin-syntax-import-attributes": "7.27.1",
    "@babel/plugin-syntax-jsx": "7.27.1",
    "@babel/parser": "7.27.4",
    "@jsenv/exception": "1.2.1",
    "@jsenv/assert": "4.5.3",
    "@jsenv/core": "40.8.0",
    "@jsenv/eslint-config-relax": "1.6.6",
    "@jsenv/github-release-package": "1.6.31",
    "@jsenv/package-publish": "1.11.32",
    "@jsenv/performance-impact": "4.4.31",
    "@jsenv/server": "16.1.2",
    "@jsenv/snapshot": "2.15.12",
    "@jsenv/test": "3.7.7",
    "playwright": "1.52.0",
    "eslint": "9.26.0",
    "prettier": "3.5.3",
    "prettier-plugin-organize-imports": "4.1.0"
  }
}
