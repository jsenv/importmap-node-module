{
  "name": "@jsenv/importmap-node-module",
  "version": "5.2.0",
  "description": "Generate importmap for node_modules",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jsenv/importmap-node-module"
  },
  "engines": {
    "node": ">=16.13.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./index.js",
  "exports": {
    ".": {
      "import": "./index.js"
    },
    "./*": "./*"
  },
  "files": [
    "/src/",
    "/index.js"
  ],
  "scripts": {
    "eslint": "npx eslint . --ext=.js,.mjs,.cjs,.html",
    "importmap": "node ./scripts/importmap.mjs",
    "performance": "node --expose-gc ./scripts/performance.mjs --log",
    "test": "node ./scripts/test.mjs",
    "test:coverage": "npm run test -- --coverage",
    "prettier": "prettier --write ."
  },
  "dependencies": {
    "@babel/core": "7.21.3",
    "@babel/traverse": "7.21.3",
    "@jsenv/filesystem": "4.1.9",
    "@jsenv/importmap": "1.2.1",
    "@jsenv/logger": "4.1.1",
    "@jsenv/urls": "1.2.8",
    "parse5": "7.1.2"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.21.3",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-proposal-decorators": "7.21.0",
    "@babel/plugin-syntax-jsx": "7.18.6",
    "@jsenv/assert": "2.8.1",
    "@jsenv/babel-preset": "1.1.3",
    "@jsenv/core": "30.4.1",
    "@jsenv/eslint-config": "16.3.4",
    "@jsenv/github-release-package": "1.5.1",
    "@jsenv/importmap-eslint-resolver": "5.2.5",
    "@jsenv/package-publish": "1.10.1",
    "@jsenv/performance-impact": "4.1.0",
    "@jsenv/server": "14.1.15",
    "eslint": "8.36.0",
    "eslint-plugin-html": "7.1.0",
    "eslint-plugin-import": "2.27.5",
    "prettier": "2.8.4"
  }
}
