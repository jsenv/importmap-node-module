{
  "name": "@jsenv/importmap-node-module",
  "version": "7.0.0",
  "description": "Generate importmap for node_modules",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jsenv/importmap-node-module"
  },
  "bin": "./src/cli.mjs",
  "engines": {
    "node": ">=16.17.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./src/main.js",
  "exports": {
    ".": {
      "import": "./src/main.js"
    },
    "./*": "./*"
  },
  "files": [
    "/src/"
  ],
  "workspaces": [
    "./docs/demo"
  ],
  "scripts": {
    "eslint": "npx eslint . --ext=.js,.mjs,.cjs,.html",
    "test": "node ./scripts/test.mjs",
    "prettier": "prettier --write .",
    "performance": "node --expose-gc ./scripts/performance.mjs --log"
  },
  "dependencies": {
    "@babel/core": "7.24.9",
    "@babel/traverse": "7.24.8",
    "@jsenv/ast": "6.2.5",
    "@jsenv/filesystem": "4.9.1",
    "@jsenv/importmap": "1.2.1",
    "@jsenv/logger": "4.1.1",
    "@jsenv/urls": "2.3.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.24.8",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-proposal-decorators": "7.24.7",
    "@babel/plugin-syntax-import-attributes": "7.24.7",
    "@babel/plugin-syntax-jsx": "7.24.7",
    "@jsenv/assert": "4.1.5",
    "@jsenv/babel-preset": "1.1.3",
    "@jsenv/core": "39.2.5",
    "@jsenv/eslint-config": "16.5.2",
    "@jsenv/github-release-package": "1.5.5",
    "@jsenv/importmap-eslint-resolver": "5.2.5",
    "@jsenv/package-publish": "1.10.5",
    "@jsenv/performance-impact": "4.2.1",
    "@jsenv/server": "15.2.14",
    "@jsenv/snapshot": "2.2.5",
    "@jsenv/test": "3.3.16",
    "eslint": "8.57.0",
    "eslint-plugin-html": "8.1.1",
    "eslint-plugin-import": "2.29.1",
    "prettier": "3.3.3",
    "prettier-plugin-organize-imports": "4.0.0"
  }
}
